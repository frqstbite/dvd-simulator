{
  "name": "dvd-simulator",
  "tree": {
    "$className": "DataModel",

    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    },
    
    "ReplicatedFirst": {
      "Packages": {
        "$path": "Packages"
      }
    },

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterGui": {
      "$properties": {
        "ScreenOrientation": "LandscapeSensor"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    }
  }
}